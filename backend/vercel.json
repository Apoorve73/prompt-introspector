{
    "version": 2,
    "builds": [
        {
            "src": "server-vercel.js",
            "use": "@vercel/node"
        }
    ],
    "routes": [
        {
            "src": "/api/(.*)",
            "dest": "/server.js"
        },
        {
            "src": "/(.*)",
            "dest": "/server.js"
        }
    ],
    "functions": {
        "server.js": {
            "runtime": "nodejs18.x"
        }
    },
    "env": {
        "NODE_ENV": "production"
    }
}