{
    "name": "prompt-introspector-backend",
    "version": "1.0.0",
    "description": "Backend proxy server for OpenAI API integration with Prompt Introspector",
    "main": "src/server.js",
    "type": "module",
    "scripts": {
        "start": "node src/server.js",
        "dev": "nodemon src/server.js",
        "test": "jest",
        "test:watch": "jest --watch",
        "test:coverage": "jest --coverage",
        "format": "prettier --write \"src/**/*.{js,json,md}\"",
        "format:check": "prettier --check \"src/**/*.{js,json,md}\"",
        "lint": "eslint src --ext .js",
        "lint:fix": "eslint src --ext .js --fix",
        "build": "echo 'No build step required for Node.js'",
        "clean": "rm -rf node_modules package-lock.json",
        "postinstall": "echo 'Backend dependencies installed successfully'"
    },
    "keywords": [
        "openai",
        "proxy",
        "ai",
        "prompt",
        "introspector",
        "express",
        "cors",
        "rate-limiting",
        "security",
        "logging"
    ],
    "author": "Your Name",
    "license": "MIT",
    "dependencies": {
        "express": "^4.18.2",
        "cors": "^2.8.5",
        "express-rate-limit": "^7.1.5",
        "express-validator": "^7.0.1",
        "dotenv": "^16.3.1",
        "openai": "^4.20.1"
    },
    "devDependencies": {
        "nodemon": "^3.0.1",
        "jest": "^29.7.0",
        "supertest": "^6.3.3",
        "eslint": "^8.55.0",
        "prettier": "^3.1.0"
    },
    "engines": {
        "node": ">=18.0.0",
        "npm": ">=9.0.0"
    },
    "repository": {
        "type": "git",
        "url": "https://github.com/yourusername/prompt-introspector.git"
    },
    "bugs": {
        "url": "https://github.com/yourusername/prompt-introspector/issues"
    },
    "homepage": "https://github.com/yourusername/prompt-introspector#readme",
    "jest": {
        "testEnvironment": "node",
        "collectCoverageFrom": [
            "src/**/*.js",
            "!src/server.js"
        ],
        "coverageDirectory": "coverage",
        "coverageReporters": [
            "text",
            "lcov",
            "html"
        ]
    }
}
